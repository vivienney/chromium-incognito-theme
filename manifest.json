{
	"manifest_version": 2,

	"name": "Chromium Incognito",
	"description": "This theme is based on Chromium incognito dark theme.",

	"version": "5.3.0",

	"icons": {
		"128": "icons/icon128.png"
	},

	"theme": {
		"images": {
			"theme_button_background": "images/theme_button_background.png",
			"theme_frame": "images/frame_default.png",
			"theme_frame_incognito": "images/frame_default.png",
			"theme_frame_inactive": "images/frame_default.png",
			"theme_frame_incognito_inactive": "images/frame_default.png",
			"theme_tab_background": "images/theme_tab_default.png",
			"theme_tab_background_incognito": "images/theme_tab_default.png",
			"theme_toolbar": "images/theme_toolbar.png"
		},
		"colors": {
			"frame": [32, 33, 36],
			"frame_inactive": [32, 33, 36],
			"frame_incognito": [32, 33, 36],
			"frame_incognito_inactive": [32, 33, 36],
			"toolbar": [53, 54, 58],
			"tab_text": [255, 255, 255],
			"tab_background_text": [189, 193, 198],
			"tab_background_text_incognito": [189, 193, 198],
			"tab_background_text_inactive": [168, 171, 175],
			"tab_background_text_incognito_inactive": [168, 171, 175],
			"bookmark_text": [255, 255, 255],
			"ntp_background": [53, 54, 58],
			"ntp_text": [232, 234, 237],
			"omnibox_background": [32, 33, 36],
			"omnibox_text": [255, 255, 255]
		},
		"tints": {
			"buttons": [-1, -1, 0.96]
			"frame": [-1, -1, -1],
			"frame_inactive": [-1, -1, -1],
			"frame_incognito": [-1, -1, -1],
			"frame_incognito_inactive": [-1, -1, -1]
		},
		"properties": {
			"ntp_logo_alternate": 1,
			"ntp_background_repeat": "repeat"
		}
	}
}
